{
  "name": "@yimoko/store",
  "version": "2.0.19",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ickeep/yimoko-store.git"
  },
  "license": "MIT",
  "author": {
    "name": "ickeep",
    "email": "i@ickeep.com",
    "url": "https://github.com/ickeep"
  },
  "exports": {
    ".": {
      "import": "./dist/yimoko-store.es.js",
      "require": "./dist/yimoko-store.cjs.js",
      "default": "./src/library.ts"
    }
  },
  "main": "src/library.ts",
  "types": "types/library.d.ts",
  "files": [
    "src",
    "dist",
    "types",
    "README.md"
  ],
  "scripts": {
    "build": "react-scripts build",
    "library": "vite build",
    "lint": "eslint ./src --fix --ext ts,tsx",
    "start": "react-scripts start",
    "test:unit": "react-scripts test  --coverage --watchAll=false",
    "test:watch": "react-scripts test --coverage --watchAll"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/index.tsx",
      "reportWebVitals",
      "/src/_demo"
    ],
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs",
      "^lodash-es$": "lodash"
    }
  },
  "dependencies": {
    "@formily/core": "^2.3.0",
    "@formily/react": "^2.3.0",
    "@formily/reactive": "^2.3.0",
    "@formily/reactive-react": "^2.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.3.4",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.2.6",
    "@types/jest": "^27.5.2",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^16.18.21",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@types/react-is": "^17.0.3",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^3.1.0",
    "antd": "^5.9.4",
    "eslint": "^8.51.0",
    "eslint-config-tencent": "^1.0.4",
    "jest": "^29.5.0",
    "moxios": "^0.4.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.1.0",
    "web-vitals": "^2.1.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}